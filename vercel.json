{
    "version": 2,
    "builds": [
      {
        "src": "src/telegram_bot_api.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/telegram_bot_api.py"
      }
    ]
}