{
    "version": 2,
    "builds": [
      {
        "src": "run.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9", "maxDuration": 60 }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "run.py"
        
      }
    ]
}